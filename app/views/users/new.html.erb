<h1>Sign UP</h1>
<%= form_for @user do |f| %>
 <%= f.label :mail %>
 <%= f.email_field :mail %><br>
 <%= f.label :password %>
 <%= f.password_field :password%><br>
 <%= f.label :password_confirmation %>
 <%= f.password_field :password_confirmation %><br>
 <%= f.submit "Register"%>
<% end %>
<%= link_to 'Back','/login' %>
